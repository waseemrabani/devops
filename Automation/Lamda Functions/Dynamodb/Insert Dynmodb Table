import boto3
from boto3.dynamodb.conditions import Key

client = boto3.client('dynamodb')
def lambda_handler(event, context):
    data = client.put_item(
    TableName='VPN',
    Item={
        'name': {
          'S': 'Routes'
        },
   
        'Routes': {
          'S': 'Update'
        },    
    
        'RT': {
          'S': 'Get'
        },
        
    }    
    
   
  )

